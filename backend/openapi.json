      "EnhancementParams": {
        "properties": {
          "use_gamma": {
            "type": "boolean",
            "title": "Use Gamma",
            "description": "Use gamma correction",
            "default": false
          },
          "gamma": {
            "type": "number",
            "title": "Gamma",
            "description": "Gamma value (<1.0 brightens, >1.0 darkens)",
            "default": 0.5
          },
          "use_clahe": {
            "type": "boolean",
            "title": "Use Clahe",
            "description": "Use CLAHE",
            "default": false
          },
          "clahe_clip": {
            "type": "number",
            "title": "Clahe Clip",
            "description": "CLAHE clip limit",
            "default": 3.0
          },
          "clahe_tile_size": {
            "anyOf": [
              {
                "items": {
                  "type": "integer"
                },
                "type": "array"
              },
              {
                "type": "null"
              }
            ],
            "title": "Clahe Tile Size",
            "description": "CLAHE tile grid size [width, height]",
            "default": [
              8,
              8
            ]
          },
          "use_ssr": {
            "type": "boolean",
            "title": "Use Ssr",
            "description": "Use Single-Scale Retinex",
            "default": false
          },
          "ssr_sigma": {
            "type": "integer",
            "title": "Ssr Sigma",
            "description": "SSR sigma value",
            "default": 80
          },
          "use_msr": {
            "type": "boolean",
            "title": "Use Msr",
            "description": "Use Multi-Scale Retinex",
            "default": false
          },
          "msr_sigmas": {
            "items": {
              "type": "integer"
            },
            "type": "array",
            "title": "Msr Sigmas",
            "description": "MSR sigma values list",
            "default": [
              15,
              80,
              250
            ]
          },
          "use_sharpen": {
            "type": "boolean",
            "title": "Use Sharpen",
            "description": "Use sharpening",
            "default": false
          },
          "sharpen_method": {
            "type": "string",
            "title": "Sharpen Method",
            "description": "Sharpening method: 'unsharp' or 'laplacian'",
            "default": "unsharp"
          },
          "sharpen_strength": {
            "type": "number",
            "title": "Sharpen Strength",
            "description": "Sharpening strength (1.0 = normal, 2.0 = strong)",
            "default": 1.0
          },
